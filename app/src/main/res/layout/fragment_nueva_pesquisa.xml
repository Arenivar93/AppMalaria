<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".CRUD.pesquisaLarvaria.NuevaPesquisaFragment">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <android.support.design.widget.TextInputLayout
            android:id="@+id/txtAnopheles12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:errorTextAppearance="@style/MyAppTheme.TextInputLayout">
            <android.support.design.widget.TextInputEditText
                android:id="@+id/tilAnopheles12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Anopheles (I-II):"/>
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/txtAnopheles34"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:errorTextAppearance="@style/MyAppTheme.TextInputLayout">
            <android.support.design.widget.TextInputEditText
                android:id="@+id/tilAnopheles34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Anopheles (III-IV):"/>
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/txtCulicino12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:errorTextAppearance="@style/MyAppTheme.TextInputLayout">
            <android.support.design.widget.TextInputEditText
                android:id="@+id/tilCulicino12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Anopheles (I-II):"/>
        </android.support.design.widget.TextInputLayout>

    </LinearLayout>

</FrameLayout>